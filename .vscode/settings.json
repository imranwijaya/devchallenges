{
  "editor.tabSize": 2,
  "files.eol": "\n",
  "tailwindCSS.experimental.classRegex": [
    ["ClassName \\=([^;]*);", "'([^']*)'"],
    ["ClassName \\=([^;]*);", "\"([^\"]*)\""],
    ["ClassName \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dotenv.enableAutocloaking": false
}
